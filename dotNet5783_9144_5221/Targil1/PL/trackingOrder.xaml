<Window x:Class="PL.trackingOrder"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="trackingOrder" Height="450" Width="800">
    <Grid>
        <Label Name="Id" Content="{Binding Path=Id, Mode=TwoWay}"  HorizontalAlignment="Left" Margin="54,36,0,0" VerticalAlignment="Top" Height="26" Width="74" Background="#FFC7C7C7"/>
        <Label Name="CustomerName" Content="{Binding Path=CustomerName, Mode=TwoWay}"  HorizontalAlignment="Left" Margin="55,74,0,0" VerticalAlignment="Top" Height="55" Width="95" Background="#FFCAC2C2"/>
        <Label Name="CustomerEmail" Content="{Binding Path=CustomerEmail, Mode=TwoWay}"  HorizontalAlignment="Left" Margin="55,145,0,0" VerticalAlignment="Top" Height="55" Width="95" Background="#FFC1B9B9"/>



    </Grid>
</Window>
